<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.catstagram.mapper.CommentMapper">
	<!-- 피드(게시글) 댓글 작성 -->
	<insert id="feedCommentInsert" parameterType="com.catstagram.comment.model.CommentDTO">
		insert into
			catstagram_comment
		values
			(catstagram_comment_idx.nextval, #{feed_idx}, #{member_idx}, #{feed_content}, 0, sysdate)
	</insert>
</mapper>